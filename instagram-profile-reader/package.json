{
  "name": "instagram-profile-reader",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev:full": "concurrently \"npm run start:server\" \"npm run dev --prefix frontend\"",
    "start:server": "node --loader ts-node/esm src/server.ts",
    "dev": "node --loader ts-node/esm src/index.ts",
    "build": "tsc --project tsconfig.json",
    "test": "vitest run",
    "lint": "eslint . --ext .ts"
  },
  "dependencies": {
    "colord": "^2.9.3",
    "commander": "^12.1.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^5.1.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/node": "^22.10.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vitest": "^1.6.0"
  }
}
